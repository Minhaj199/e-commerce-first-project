    <div class="row first-row">
        <div style="padding: 0;" class="col-9 main-content">
            <div class="col-12 titile-box">
                <h2>Order Product Datails</h2>
            </div>
            <div style="" class="continer-fuid main-cont-inner">
                <div class="col-12 address ">

                    <div class="address-box">

                        <div style="" class="col-2 button-box">
                            <h5>Billed Address</h5>

                        </div>
                        <div style="" class="col-10 address-inner-box">
                            <h3 style="margin-top: 2%;font-weight: bolder;">{{orderData.[0].address.Name}}</h3>
                            <p style="color: black;margin-top: 1%;">
                                Phone:{{orderData.[0].address.Mobile}}&nbsp;&nbsp;&nbsp;Aleternative
                                mobile:{{orderData.[0].address.Aleternative_mobile}} </p>
                            <p style="color: black;margin-top: 1%;">
                                Town:{{orderData.[0].address.Town}}&nbsp;&nbsp;Locality:{{orderData.[0].address.Locality}}&nbsp;&nbsp;Land
                                mark:{{orderData.[0].address.Land_mark}}</p>
                            <p style="color: black;margin-top: 1%;">Adress:{{orderData.[0].address.Address}}</p>
                        </div>





                    </div>
                    <div style="height: 50px;width:80%;margin-top: 2%; ">

                    </div>
                </div>

                <div id="oderId" class="col-11  order">
                    <div style="height:100px;display: flex;justify-content: center;align-items: center;" class="col-12">
                        <h2>Order Detail</h2>
                    </div>

                    <div class="col-12" style="height: 500px;overflow: auto;padding: 0; ">

                        <table style="background-color: rgb(233, 233, 233);width: 100%;">
                            <thead style="border-bottom: 1px solid rgb(144, 143, 143);">
                                <tr>
                                    <th>NO</th>
                                    <th>Product</th>
                                    <th style="width: 20%;">Name</th>
                                    <th class="padding-right">Color</th>
                                    <th class="padding-right">Size</th>
                                    <th class="padding-right">Price</th>
                                    <th class="padding-right">QTY</th>
                                    <th class="padding-right">Total</th>
                                    <th>Status</th>

                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each ProductData}}
                                <tr>

                                    <td>{{increment @index}}</td>
                                    <td><img style="width: 100px;" src="{{this.ProductDetails.images.path.[0]}}" alt="">
                                    </td>
                                    <td class="name">{{this.ProductDetails.Name}}</th>

                                    <td class="All-total">{{this.Order.color}}</td>
                                    <td class="All-total">{{this.Order.size}}</td>
                                    <td class='price'>{{this.Order.price}}</td>

                                    <td class='quatity'>{{this.Order.quantity}}</td>
                                    <td class='quatity'>{{this.Order.total}}</td>
                                    {{#isEqual this.Order.status "Requested for Return"}}
                                    <td style="cursor: pointer;color:red" onclick='showReason("{{this.Order.reason}}")'>{{this.Order.status}}</td>
                                    {{else}}
                                    <td  onclick='showReason("{{this.Order.reason}}")'>{{this.Order.status}}</td>
                                    {{/isEqual}}
                                    <div id="my-modalin" class="modalin">
                                        <div class="contene-modalin">
                                            <span class="close">&times;</span>
                                            <p>Reason:</p>
                                            <h3 id="reason-matter">{{this.Order.reason}}</h3>
                                            
                                        </div>
                                    </div>
                                    <td>
                                    {{#if this.Order.admin}}
                                    {{#isEqual this.Order.status "Requested for Return"}}
                                        <select name="ReturnStatus" class="drop" onchange='productReturn(this.value, "{{this._id}}",{{@index}})' >
                                            <option >Manage Return</option>
                                            <option value="Return Accept">Accept</option>
                                            <option value="Return Reject">Reject</option>
                                        
                                            
                                        
                                        </select>
                                    {{else}}
                                    
                                    <select name="orderStatus" class="drop" onchange='ChangeStatus(this.value, "{{this._id}}",{{@index}})' >
                                        <option value="Pending">Manage Status</option>
                                        <option value="Pending">Pending</option>
                                        <option value="Delivered">Delivered</option>
                                    
                                        <option value="Canceled">Cancel</option>
                                    
                                    </select>
                                    {{/isEqual}}
                                    {{/if}}
                                    </td>
                                </tr>
                                {{/each}}
                                <input id="orderID" value="{{orderData.[0]._id}}" type="text" hidden>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>


        </div>
        <div style="padding-left: 19px;" class="col-3 billing-colum">
            <div style="width: 100%;height: 50%;background-color: rgb(226, 226, 226);margin-top: 23%;">
                <div style="height: 10%;display: flex;justify-content: center;align-items: center;" class="col-12">
                    <h2 style="font-family: Georgia, 'Times New Roman', Times, serif;">Your Order</h2>

                </div>
                <div style="height: 80%;" class="col-12">

                    <table>
                        <thead>
                            <tr>
                                <th style="padding-left:1px ;color:#880606">Sub Totoal</th>
                                <th id="add-sub-total" style="padding-left: 120px; color: black;">
                                    {{orderData.[0].SubTotal}}</th>

                            </tr>
                            <tr>
                                <th class="final-order-heading">Discount</th>
                                <th id="discount" style="padding-left: 120px;padding-top:20px;color: black    ;">
                                    {{orderData.[0].Discount}}</th>

                            </tr>
                            <tr style="border-bottom: 1px solid rgb(181, 179, 179);">
                                <th class="final-order-heading">Shipping Charge</th>
                                <th id="shipping-charge" style="padding-left: 120px;padding-top:20px;color: black    ;">
                                    60</th>

                            </tr>
                            <tr style="border-bottom: 1px solid rgb(181, 179, 179);;">

                                <th class="final-order-heading">Total</th>
                                <th id="Total" style="padding-left: 120px;padding-top:30px;color: black    ;">
                                    {{orderData.[0].TotalOrderPrice}}</th>

                            </tr>

                            </tr>
                            <tr>
                                {{!-- {{#isEqual {{orderData.[0].PaymentOption}} 'Cash on delevery'}} --}}
                                <hr>
                                <th class="final-order-heading">Payment</th>
                                <th style="padding-left: 100px;padding-top:30px;color: black    ;word-break: break-all;">
                                    {{orderData.[0].PaymentOption}}</th>

                            </tr>

                            {{!-- {{/isEqual}} --}}
                        </thead>
                    </table>


                </div>
            </div>
        </div>
    </div>


